    "react-router-dom": "^6.30.0",
    "react-router-hash-link": "^2.4.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
